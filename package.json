{
  "name": "@zestia/ember-messages",
  "version": "2.2.0",
  "description": "Message components for Ember apps & addons",
  "keywords": [
    "ember-addon",
    "message",
    "flash"
  ],
  "license": "MIT",
  "author": "Zestia",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:css": "stylelint '**/*.{css,scss}'",
    "lint:css:fix": "stylelint '**/*.{css,scss}' --fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:ember",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prettier": "prettier . --write",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zestia/ember-messages.git"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-modifier": "^2.1.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.5",
    "@embroider/test-setup": "^0.41.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@zestia/eslint-config": "^4.0.4",
    "@zestia/prettier-config": "^1.0.5",
    "@zestia/stylelint-config": "^2.0.47",
    "@zestia/template-lint-config": "^4.1.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.0.1",
    "ember-cli": "^3.27.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "^3.27.2",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.4.2",
    "ember-try": "^1.4.0",
    "eslint": "^7.27.0",
    "eslint-plugin-qunit": "^6.1.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "qunit": "^2.15.0",
    "qunit-dom": "^1.6.0",
    "release-it": "^14.7.0",
    "sass": "^1.34.0",
    "stylelint": "^13.13.1",
    "webpack": "^5.38.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
