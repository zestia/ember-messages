{
  "name": "@zestia/ember-messages",
  "version": "0.1.0",
  "description": "Message components for Ember apps & addons",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm run lint:js && npm run lint:hbs && npm run lint:css",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:css": "stylelint --allow-empty-input '**/*.{css,scss}'",
    "prettier": "prettier '**/*.{js,json,css,scss,html,md}' --write",
    "start": "ember serve",
    "test": "ember test && npm run lint",
    "test:all": "ember try:each"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zestia/ember-messages.git"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "author": "Zestia",
  "license": "MIT",
  "devDependencies": {
    "@ember/optional-features": "1.0.0",
    "@zestia/ember-template-lint-plugin": "2.0.4",
    "@zestia/eslint-config": "2.0.28",
    "@zestia/prettier-config": "0.0.5",
    "@zestia/stylelint-config": "2.0.22",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-github-pages": "0.2.2",
    "ember-cli-inject-live-reload": "2.0.2",
    "ember-cli-release-tag": "0.1.5",
    "ember-cli-sass": "10.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-test-loader": "2.2.0",
    "ember-cli-uglify": "3.0.0",
    "ember-cli": "3.13.1",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-load-initializers": "2.1.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.5.1",
    "ember-resolver": "5.3.0",
    "ember-source-channel-url": "2.0.1",
    "ember-source": "3.13.3",
    "ember-template-lint": "1.5.3",
    "ember-try": "1.2.1",
    "eslint": "6.5.1",
    "loader.js": "4.7.0",
    "prettier": "1.18.2",
    "qunit-dom": "0.9.0",
    "sass": "1.23.0",
    "stylelint": "11.0.0"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "ember-cli-babel": "^7.11.1",
    "ember-cli-htmlbars": "^4.0.0"
  },
  "keywords": [
    "ember-addon",
    "message",
    "flash"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://zestia.github.io/ember-messages"
  },
  "bugs": {
    "url": "https://github.com/zestia/ember-messages/issues"
  },
  "homepage": "https://github.com/zestia/ember-messages",
  "configPath": "tests/dummy/config"
}
